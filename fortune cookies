import random
lucky_number = random. randint (1,10)


fortune_number = random. randint (1,2)

fortune_text=''

if fortune_number == 1:
  fortune_text = 'you will have a great day!'
if fortune_number == 2:
  fortune_text = 'you will have a tough day!'

print(f'{fortune_text} your lucky number is: {lucky_number}')
